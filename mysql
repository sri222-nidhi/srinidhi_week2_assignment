create TABLE Signup(
id INT AUTO_INCREMENT,
Username VARCHAR(255),
Email VARCHAR(255),
Password VARCHAR(255),
PRIMARY KEY(id)
);
select * from signup ;

create TABLE Login(
id INT AUTO_INCREMENT,
Email VARCHAR(255),
Password VARCHAR(255),
PRIMARY KEY(id)
);
select * from Login;



